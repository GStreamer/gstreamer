msgid "performer"
msgstr "виконавець"

#: gst/gsttaglist.c:219
msgid "person(s) performing"
msgstr "особа(и), що виконала"

#: gst/gsttaglist.c:222
msgid "composer"
msgstr "композитор"

#: gst/gsttaglist.c:223

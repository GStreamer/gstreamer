set-globals, media_dir="$(test_dir)/../../../media"
meta,
    tool = "gst-validate-$(gst_api_version)",
    args = {
         "playbin uri=file://$(media_dir)/defaults/mp4/av1-image-sequence.mp4 name=playbin video-sink=\"$(videosink) name=videosink\"",
    },
    configs = {
       "$(validateflow), pad=qtdemux0:video_0, record-buffers=true, buffers-checksum=sha1",
    },
    scenario="play_15s"


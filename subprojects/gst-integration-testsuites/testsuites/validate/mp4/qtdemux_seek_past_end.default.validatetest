set-globals, media_dir="$(test_dir)/../../../medias/"
meta,
    reverse-playback=true,
    seek=true,
    handles-states=true,
    args = {
         "filesrc location=$(media_dir)/defaults/mp4/raw_h264.0.mp4 ! qtdemux ! audio/x-raw ! fakesink name=fakesink",
    },
    configs = {
       "$(validateflow), pad=fakesink:sink, record-buffers=true",
    }

pause
seek, name=Reverse-seek, playback-time=0.0, rate=1.0, start=30.0, flags="flush"
play

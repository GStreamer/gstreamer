set-globals, media_dir="$(test_dir)/../../../medias/"
meta,
    handles-states=true,
    ignore-eos=true,
    args = {
        "playbin3 uri=file://$(media_dir)/big/matroska/Sintel.2010.720p.mkv video-sink=\"$(videosink)\" audio-sink=\"$(audiosink)\"",
    }

play
seek, name="Seeking to first subtitle", playback_time=0.0, start=107.5, flags=accurate+flush
wait, duration=1.0
switch-track, type=text, index=(string)+1
wait, duration=1.0
switch-track, type=text, index=(string)+1
wait, duration=2.0
stop

set-globals, media_dir="$(test_dir)/../../../media/"
meta,
    handles-states=true,
    ignore-eos=true,
    args = {
        "playbin3 uri=file://$(media_dir)/defaults/matroska/test5.mkv video-sink=\"$(videosink)\" audio-sink=\"$(audiosink)\"",
    }

play
wait, duration=1.0
switch-track, type=text, index=(string)+1
wait, duration=1.0
switch-track, type=text, index=(string)+1
wait, duration=4.0
stop
